{

  "manifest_version": 2,
  "name": "jsFlashPlayer",
  "version": "1.0",
 
  "description": "Flash Player written on pure JavaScript. Yes this is stupid idea, but why not?",
  "icons": {
    "48": "icons/icon-48.png"
  },

  "applications": {
    "gecko": {
      "id": "jsFlashPlayer@mozilla.org",
      "strict_min_version": "45.0"
    }
  },

  "web_accessible_resources": [
    "helpers/ffmpeg_wrapper/ffmpeg.wasm"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at" : "document_start",
      "match_about_blank": true,
      "all_frames": true,
      "js": [
        "helpers/ffmpeg_wrapper/ffmpeg_wrapper.js",
        "helpers/ffmpeg_wrapper/ffmpeg.js",
        "helpers/pako/pako_inflate.min.js",
        "helpers/lzma/lzma-d-min.js",
        "helpers/query.js",
        "helpers/debug.js",
        "flash/interface/FlashParser.js",
        "flash/AVM2.js",
        "flash/AVM.js",
        "flash/data_types/genericDrawable.js",
        "flash/data_types/Rect.js",
        "flash/data_types/SoundStream.js",
        "flash/data_types/Dictionary.js",
        "flash/data_types/DisplayList.js",
        "flash/data_types/Sound.js",
        "flash/data_types/Shape.js",
        "flash/data_types/Font.js",
        "flash/data_types/Sprite.js",
        "flash/data_types/Text.js",
        "flash/data_types/Timeline.js",
        "flash/data_types/TextEdit.js",
        "flash/data_types/VideoStream.js",
        "flash/data_types/ColorTransform.js",
        "flash/data_types/KeyboardController.js",
        "flash/tags/genericTag.js",
        "flash/tags/DefineSceneAndFrameLabelData.js",
        "flash/tags/DoABC.js",
        "flash/tags/DefineSound.js",
        "flash/tags/StartSound.js",
        "flash/tags/DefineShape.js",
        "flash/tags/DefineShape2.js",
        "flash/tags/DefineShape3.js",
        "flash/tags/DefineBitsLossless.js",
        "flash/tags/DefineBitsLossless2.js",
        "flash/tags/SoundStreamHead.js",
        "flash/tags/SoundStreamHead2.js",
        "flash/tags/DefineFont.js",
        "flash/tags/DefineFont2.js",
        "flash/tags/DefineFontInfo.js",
        "flash/tags/DefineFontInfo2.js",
        "flash/tags/DefineEditText.js",
        "flash/tags/DefineSprite.js",
        "flash/tags/DefineText.js",
        "flash/tags/PlaceObject2.js",
        "flash/tags/RemoveObject2.js",
        "flash/tags/Metadata.js",
        "flash/tags/VideoFrame.js",
        "flash/tags/DoAction.js",
        "flash/tags/DefineVideoStream.js",
        "flash/tags/DefineBitsJPEG2.js",
        "flash/tags/SetBackgroundColor.js",
        "flash/tags/ExportAssets.js",
        "flash/tags/ShowFrame.js",
        "flash/tags/End.js",
        "flash/tags/FileAttributes.js",
        "flash/tags/SoundStreamBlock.js",
        "flash/tags/Protect.js",
        "flash/tags/DefineFontName.js",
        "flash/tags/EnableDebugger2.js",
        "flash/tags/DebugID.js",
        "flash/tags/ScriptLimits.js",
        "flash/tags/ProductInfo.js",
        "flash/tags/FrameLabel.js",
        "flash/Preloader.js",
        "flash/FlashCore.js",
        "jsFlashPlayer.js"
      ]
    }
  ],
   "permissions": [
    "<all_urls>"
  ]
}

